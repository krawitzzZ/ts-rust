{
  "$schema": "https://typedoc.org/schema.json",
  "plugin": ["typedoc-plugin-markdown", "typedoc-plugin-mdn-links"],
  "entryPointStrategy": "resolve",
  "entryPoints": ["src/index.ts", "src/error", "src/option", "src/result"],
  "out": "docs",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeInternal": true,
  "readme": "./README.md",
}
